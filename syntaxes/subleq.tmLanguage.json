{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Subleq",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#special"
		},
		{
			"include": "#imports"
		},
		{
			"include": "#newline"
		},
		{
			"include": "#relative"
		},
		{
			"include": "#blockcomments"
		}
	],
	"repository": {

		"keywords": {
			"patterns": [{
				"name": "keyword.control.subleq",
				"match": "(-=)|!(.*?)( |\n)"
			}]
		},

		"strings": {
			"name": "string.quoted.double.subleq",
			"begin": "\"|'",
			"end": "\"|'",
			"patterns": [
				{
					"name": "constant.character.escape.subleq",
					"match": "\\\\."
				}
			]
		},
		"comments": {
			"name": "comment.line.subleq",
			"begin": ";",
			"end": "\n",
			"patterns": [
				{
					"name": "comment.line.subleq",
					"match": "\\\\."
				}
			]
		},
		"blockcomments": {
			"name": "comment.block.subleq",
			"begin": "\\*\\*",
			"end": "\\*\\*",
			"patterns": [
				{
					"name": "comment.block.subleq",
					"match": "\\\\."
				}
			]
		},
		"constants": {
			"patterns": [{
				"name": "constant.numeric.subleq",
				"match": "\\b(0x[0-9A-F]+)|[0-9]+|[A-Z][A-Z_]*\\b"
			}]
		},
		"functions": {
			"patterns": [{
				"name": "entity.name.function.subleq",
				"match": "(-.*?>)|(@(.*?)( |\n))"
			}]
		},
		"operators": {
			"patterns": [{
				"name": "keyword.operator.subleq",
				"match": "="
			}]
		},

		"special": {
			"patterns": [{
				"name": "invalid",
				"match": "::"
			}]
		},
		"imports": {
			"name": "entity.name.class.subleq",
			"patterns": [
				{
					"name": "entity.name.class.subleq",
					"match": "(#.+\n)|([a-z0-9_.]+\\?)|\\?"
				}
			]
		}



	},
	"scopeName": "source.sbl"
}